<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ViewLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/viewLineColor</item>
    </style>

    <style name="s_true" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#33da33</item>
    </style>

    <style name="s_false" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#9b9b9b</item>
    </style>

    <style name="WheelDefault">
        <item name="wheel_itemTextColor">@android:color/darker_gray</item>
        <item name="wheel_itemTextColorSelected">@android:color/holo_blue_dark</item>
        <item name="wheel_itemTextSize">16sp</item>
        <item name="wheel_itemTextSizeSelected">16sp</item>
        <item name="wheel_itemTextBoldSelected">false</item>
        <item name="wheel_sameWidthEnabled">false</item>
        <item name="wheel_atmosphericEnabled">true</item>
        <item name="wheel_curtainEnabled">false</item>
        <item name="wheel_curvedEnabled">false</item>
        <item name="wheel_curvedMaxAngle">90</item>
        <item name="wheel_cyclicEnabled">false</item>
        <item name="wheel_indicatorEnabled">true</item>
        <item name="wheel_indicatorColor">@android:color/holo_blue_light</item>
        <item name="wheel_indicatorSize">1dp</item>
    </style>

    <style name="WheelStyleDemo">
        <item name="wheel_itemTextColor">@android:color/holo_green_dark</item>
        <item name="wheel_itemTextColorSelected">@android:color/holo_green_light</item>
        <item name="wheel_itemTextSize">20sp</item>
        <item name="wheel_itemTextSizeSelected">22sp</item>
        <item name="wheel_itemTextBoldSelected">true</item>
        <item name="wheel_sameWidthEnabled">false</item>
        <item name="wheel_atmosphericEnabled">true</item>
        <item name="wheel_curtainEnabled">false</item>
        <item name="wheel_curvedEnabled">false</item>
        <item name="wheel_curvedMaxAngle">60</item>
        <item name="wheel_cyclicEnabled">true</item>
        <item name="wheel_indicatorEnabled">true</item>
        <item name="wheel_indicatorColor">@android:color/holo_green_dark</item>
        <item name="wheel_indicatorSize">1dp</item>
    </style>

    <style name="TreeNodeStyleCustom">
        <item name="android:paddingLeft">20dp</item>
        <!--<item name="android:showDividers">middle</item>-->
        <!--<item name="android:divider">@android:drawable/divider_horizontal_bright</item>-->
    </style>
</resources>